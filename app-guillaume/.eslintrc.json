{
    "plugins": ["prettier"],
    "env": {
        "es2021": true,
        "node": true,
        "browser": false
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "sort-imports": "error",
        "no-console": "warn",
        "prettier/prettier": "error"
    }
}
