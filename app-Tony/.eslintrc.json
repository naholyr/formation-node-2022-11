{
    "plugins": ["prettier"],
    "env": {
        "browser": false,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "prettier/prettier": "error"
    }
}
